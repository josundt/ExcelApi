{
    "compilerOptions": {
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noExternalResolve": false,
        "removeComments": false,
        "sourceMap": true,
        "sourceRoot": "",
        "outDir": "./app/",
        "module": "amd",
        "preserveConstEnums": true,
        "target": "es5",
        "listFiles": true,
        "emitDecoratorMetadata": true
    },
    "filesGlob": [
        "./appsrc/**/*.ts",
        "./typings/aurelia/**/*.ts"
    ]
}